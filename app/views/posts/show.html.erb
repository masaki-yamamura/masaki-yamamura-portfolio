<div class="mybooks-contents" id="mybooks">
  <div class="section-title">
    <h3>Read Books</h3>
  </div>
  <div class="books-box">
    <% @posts.each do |post|%>
      <div class="books">
        <%= image_tag "#{post.image}",class: "photo-book" %>
        <p><%= post.text %></p>
      </div>
    <% end %>
  </div>
</div>